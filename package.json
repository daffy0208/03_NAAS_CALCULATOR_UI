{
  "name": "naas-pricing-calculator",
  "version": "1.0.0",
  "description": "Network-as-a-Service Pricing Calculator - Professional web application for pricing network services and equipment",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "lint": "eslint src --ext .js,.ts --fix",
    "lint:check": "eslint src --ext .js,.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "husky install"
  },
  "keywords": [
    "naas",
    "network-as-a-service",
    "pricing-calculator",
    "prtg",
    "network-monitoring",
    "spreadsheet-replication",
    "business-calculator"
  ],
  "author": "Qolcom Ltd",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-legacy": "^5.3.1",
    "@vitest/ui": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-security": "^2.1.1",
    "husky": "^9.0.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "dompurify": "^3.0.8",
    "jspdf": "^2.5.1",
    "xlsx": "^0.18.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}